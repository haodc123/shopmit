<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape
            android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/blue_global" />
            <solid android:color="@color/gray_bright_global" />

        </shape>
    </item>

    <item android:bottom="1dp" android:left="1dp" android:top="1dp" android:right="1dp">
        <shape
            android:shape="rectangle">
            <stroke android:bottom="1dp" android:left="1dp" android:top="1dp" android:right="1dp" android:color="@color/blue_global" />
            <solid android:color="@color/gray_bright_global" />
        </shape>
    </item>
</layer-list>